@page
@model ShinobiRanks1._0.Pages.hvillage.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1 class="text-center">Hidden Villages</h1>

<section class="text-center">

    @if (User.IsInRole("kage"))
    {
        <section class="text-center">
            <h4>You are your village's last line of defense.</h4>
            <p>I know you can do it because the power lies dormant within you.</p>
            <img src="https://vignette.wikia.nocookie.net/animeandmangauniverse/images/7/7d/Naruto_Nakama_Anime.png/revision/latest?cb=20130927172513" alt="Alternate Text" />
        </section>
    }
    else if (User.IsInRole("jounin"))
    {
        <section class="text-center">
            <h4>Hello, Jounin!</h4>
            <p>I know you can do it because the power lies dormant within you.</p>
            <img src="data:image/jpeg;base64," alt="Alternate Text" />
        </section>
    }
    else if (User.IsInRole("chuunin"))
    {
        <section class="text-center">
            <h4>We need Ina-Shika-Cho.</h4>
            <p>I know you can do it because the power lies dormant within you.</p>
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS1i6wpI8V2Bwi2hLkdOoJiaUJufSRSQcWyNj6lQTzOc-Y2IbWfLQ&s" alt="Alternate Text" />
        </section>
    }
    else if (User.IsInRole("genin"))
    {
        <section class="text-center">
            <h4>This is going to be a special year of genin I see.</h4>
            <p>You are going to be great, BELIEVE IT.</p>
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTJXU_kUBfLHAuvfKhHygrjY2VZ--Ijt2cR4XiOqE5ia9r-FKP7Pw&s" alt="Alternate Text" />
        </section>
    }
    else
    {
        <section class="text-center">
            <h4>Why are you here, new comer.</h4>
            <p>We don't just allow anyoone to set foot in our village you know.</p>
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRydZnS5TiqsncmB_yXhuDOC5-DC9RHiu9BIVqnBShkRtzAIU-a7Q&s" alt="Alternate Text" />
        </section>
    }
</section>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.HVillage[0].HVName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HVillage[0].HVTerrain)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HVillage[0].KageType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.HVillage) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HVName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HVTerrain)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KageType)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.HVID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.HVID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.HVID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

<a href="/Index"><---Go Home...Now</a>
